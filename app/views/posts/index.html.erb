<% @posts.each do |post| %>
  <p class="posts"><%= post.message %>  <%= link_to 'Edit', edit_post_path(post) %> <%= link_to 'Delete', post_path(post), method: :delete,
              data: { confirm: 'Are you sure?' } %></p>
<% end %>

<%= link_to new_post_path do %>
  New post
<% end %>

<%= link_to "Sign out", destroy_user_session_path,
              method: :get,
              data: { confirm: 'Are you sure?' } %>
